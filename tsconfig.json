{
	"compilerOptions": {
		"target": "es5",
		"module": "esnext",
		"sourceMap": true,
		"outDir": "dist",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"lib": [
			"dom",
			"dom.iterable",
			"esnext"
		],
		"allowJs": true,
		"allowSyntheticDefaultImports": true,
		"noFallthroughCasesInSwitch": true,
		"moduleResolution": "node",
		"resolveJsonModule": true,
	},
	"include": [
		"kadai-9/**/*",
		"kadai-10-1/**/*",
		"kadai-10-2/**/*"
	],
	"exclude": [
		"dist",
		"node_modules"
	],
}